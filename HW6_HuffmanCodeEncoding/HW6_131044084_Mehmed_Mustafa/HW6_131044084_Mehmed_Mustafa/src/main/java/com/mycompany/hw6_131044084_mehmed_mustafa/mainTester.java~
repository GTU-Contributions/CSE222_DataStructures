            }
            else if(i == 999999){
                tEnd = System.currentTimeMillis();
                System.out.println("Veri boyutu: 1000000, Time: " + (tEnd-tStart) + " ms");                
            }
            else if(i == 9999999){
                tEnd = System.currentTimeMillis();
                System.out.println("Veri boyutu: 10000000, Time: " + (tEnd-tStart) + " ms");                
            }
        }
        System.out.println("~~~~~ Binary Search Tree Queue Timers ~~~~~");
        System.out.println(myBST.size());
        
        tStart = System.currentTimeMillis();
        for(int i=0; i<myBST.size(); ++i)
        {
            if(myBST.peek() != null)
                myBST.poll();
        }
        tEnd = System.currentTimeMillis();
        System.out.println("Veri boyutu: 10000000, Time: " + (tEnd-tStart) + " ms");
        
    }
