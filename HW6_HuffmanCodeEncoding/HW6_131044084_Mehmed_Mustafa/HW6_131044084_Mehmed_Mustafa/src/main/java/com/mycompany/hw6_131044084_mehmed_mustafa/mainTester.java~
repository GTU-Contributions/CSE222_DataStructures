        PriorityQueueBST<Integer> myBST = new PriorityQueueBST();
        System.out.println("~~~~~ Binary Search Tree Queue Timers ~~~~~");
        tStart = System.currentTimeMillis();
        for(int i=0; i<10000000; ++i)
        {
            randomVal = randomNumberGenerator.nextInt(1000000000);
            myBST.offer(randomVal);
            
            if(i == 0){
                tEnd = System.currentTimeMillis();
                System.out.println("Veri boyutu: 1, Time: " + (tEnd-tStart) + " ms");
            }
            else if(i == 9){
                tEnd = System.currentTimeMillis();
                System.out.println("Veri boyutu: 10, Time: " + (tEnd-tStart) + " ms");                
